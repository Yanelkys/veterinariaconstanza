<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
     <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list> 
    <servlet>
        <servlet-name>ClienteControl</servlet-name>
        <servlet-class>org.cfg.uapa.java.veterinaria.controles.ClienteControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorControles</servlet-name>
        <servlet-class>org.cfg.uapa.java.veterinaria.controles.DoctorControles</servlet-class>
    </servlet>
        <servlet-mapping>
        <servlet-name>ClienteControl</servlet-name>
        <url-pattern>/ClienteControl</url-pattern>
    </servlet-mapping>
            <servlet-mapping>
        <servlet-name>DoctorControles</servlet-name>
        <url-pattern>/DoctorControles</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
